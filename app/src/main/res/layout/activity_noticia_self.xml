    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mx.ambmultimedia.brillamexico.Foto">


    <include android:id="@+id/app_bar" layout="@layout/app_bar" />

    <WebView
        android:layout_below="@+id/app_bar"
